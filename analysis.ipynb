      "source": [
        "import os\n",
        "\n",
        "filenames = [\n",
        "    \"file1.txt\",\n",
        "    \"file2.txt\",\n",
        "    \"file3.txt\",\n",
        "]\n",
        "\n",
        "for filename in filenames:\n",
        "    with open(os.path.join(\"data\", filename)) as file:  \n",
        "        data = file.read()\n",
        "        print(len(data))"
      ],
